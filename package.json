{
  "name": "nestjs-boilerplate",
  "version": "1.0.0",
  "description": "Nestjs Boilerplate for API service",
  "license": "MIT",
  "scripts": {
    "dev": "node dist/server",
    "webpack": "webpack --config webpack.config.js",
    "start": "NODE_ENV=development ts-node src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" "
  },
  "dependencies": {
    "@nestjs/common": "6.4.0",
    "@nestjs/core": "6.4.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "6.4.0",
    "@nestjs/platform-socket.io": "^6.4.0",
    "@nestjs/websockets": "^6.4.0",
    "dotenv": "^8.0.0",
    "joi": "^14.3.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "typescript": "3.5.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.4.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.15",
    "@types/joi": "^14.3.3",
    "@types/node": "10.14.10",
    "@types/passport-jwt": "^3.0.1",
    "@types/socket.io": "^2.1.2",
    "@types/express": "4.16.1",
    "@types/supertest": "2.0.7",
    "jest": "^24.8.0",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tslint": "5.18.0",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5",
    "webpack-node-externals": "1.7.2",
    "concurrently": "^4.1.0",
    "nodemon": "1.18.11",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "tsconfig-paths": "3.8.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  }
}
